<web-app>
 	<display-name>scimproxy</display-name>
 	
 	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	

	<servlet>
		<servlet-name>Compliance</servlet-name>
		<servlet-class>info.simplecloud.scimproxy.compliance.Compliance</servlet-class>
	</servlet>
	<servlet-mapping>
